{
  "connection": "default",
  "collectionName": "accounts",
  "info": {
    "name": "Cuenta"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "alternative_email": {
      "type": "email"
    },
    "phone": {
      "type": "biginteger"
    },
    "alt_phone": {
      "type": "biginteger"
    },
    "web": {
      "type": "string"
    },
    "observations": {
      "type": "text"
    },
    "Contacts": {
      "collection": "contact"
    },
    "account_type": {
      "model": "account-type"
    },
    "opportunities": {
      "collection": "opportunity",
      "via": "account"
    },
    "organizacion": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
