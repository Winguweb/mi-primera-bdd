{
  "connection": "default",
  "collectionName": "contacts",
  "info": {
    "name": "Contacto"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "identification": {
      "type": "biginteger"
    },
    "charge": {
      "type": "string"
    },
    "occupation": {
      "type": "string"
    },
    "birth_date": {
      "type": "date"
    },
    "email": {
      "type": "email"
    },
    "alt_email": {
      "type": "email"
    },
    "phone": {
      "type": "biginteger"
    },
    "cellphone": {
      "type": "biginteger"
    },
    "subscribed": {
      "type": "boolean"
    },
    "observations": {
      "type": "text"
    },
    "volunteer": {
      "type": "boolean"
    },
    "account": {
      "model": "account"
    },
    "origin": {
      "model": "origin"
    },
    "contact_type": {
      "model": "contact-type"
    },
    "Genero": {
      "type": "enumeration",
      "enum": [
        "masculino",
        "femenino",
        "no_binario",
        "prefiero_no_decirlo"
      ]
    },
    "organizacion": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
